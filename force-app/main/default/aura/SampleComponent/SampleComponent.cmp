<aura:component controller="HttpCalloutCtrl">
    <aura:attribute name="ListOfCurrency" type="String[]"/>
    <aura:attribute name="response" type="Map"/>
	<div class="slds-page-header">
        <div>
            <p class="slds-page-header__title slds-truncate" title="Foreign Exchange Rates"> Foreign Exchange Rates by Azar</p>
            
            <lightning:button variant="brand" label="Foreign Exchange Rates" onclick="{!c.calloutCtrl}"/>
        </div>
    </div>	
    
    <h3 class="slds-section-title--divider">Date: {!v.response.date}</h3>
    <ul class="slds-list--dotted">
    	<aura:iteration items="{!v.ListOfCurrency}" var="rateLst">
            <li>{!rateLst}</li>
        </aura:iteration>
    </ul>
</aura:component>